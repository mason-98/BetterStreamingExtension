{
    "name": "Ultrawide Extension",
    "version": "0.0.2",
    "manifest_version": 3,
    "description": "Makes video into Ultrawide format",
    "action": {
      "default_title": "Toggle!",
      "default_icon": "logo.png",
      "default_popup":  "index.html"
    },
    "permissions" :[
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end",
        "all_frames": true
      }
    ]
  }